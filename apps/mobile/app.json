{
  "expo": {
    "name": "mobile",
    "slug": "mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/codeit.png",
    "userInterfaceStyle": "light",
    "scheme": "codeit.resource.scheme",
    "splash": {
      "image": "./assets/codeit-splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/codeit.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/codeit.png"
    },
    "plugins": ["expo-router"]
  }
}
